@model IEnumerable<KernelTravelGuides.Models.Feedback>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}



 <div class="page-content-wrapper">
                    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
             
                <div class="card">
                    <div class="card-body">
                        <h4 class="mt-0 header-title">All users Feedback</h4>
                       
                        <div class="table-responsive">
                            <table class="table table-striped mb-0">
                                <thead>
                                    <tr>
                                        <th>Feedback by</th>
                                        <th>Feedback is</th>
                                        <th>Feedback is done on </th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    
                                    {
                                        
                                         <tr>
                                        
                                            <td>  @Html.DisplayFor(modelItem => item.feedback_user_name)</td>
                                            <td>  @Html.DisplayFor(modelItem => item.feedback_desc)</td>
                                           
                                            <td>  @Html.DisplayFor(modelItem => item.created_at)</td>
                                            <td>
                                                <button type="button" class="btn btn-sm btn-warning" asp-action="Edit" asp-route-id="@item.feedback_id">
                                                <i class="fas fa-edit"></i>
                                                </button>
                                                    <button type="button" class="btn btn-sm btn-primary" asp-action="Detail" asp-route-id="@item.feedback_id">
                                                    <i class="ion-android-book"></i>
                                                </button> 
                                                <button type="button" class="btn btn-sm btn-danger" asp-action="Delete" asp-route-id="@item.feedback_id">
                                                <i class="fas fa-trash-alt"></i>
                                            </button>
                                        </td>
                                    </tr>
                                    }
                                   
                                  
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div><!-- end col -->
    
        </div><!-- end row -->
    </div>
    </div>
