


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dashboard Kernel Travel Guides </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <link rel="shortcut icon" href="~/dashboard/assets/images/favicon.ico" asp-append-version="true">
    <link href="~/dashboard/assets/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" type="text/css">
    <link href="~/dashboard/assets/css/icons.css" rel="stylesheet" asp-append-version="true" type="text/css">
    <link href="~/dashboard/assets/css/style.css" rel="stylesheet" asp-append-version="true" type="text/css">

</head>
<body class="fixed-left">

    <div id="preloader">
        <div id="status">
            <div class="spinner"></div>
        </div>
    </div><!-- Begin page -->
    <div id="wrapper">
        <!-- ========== Left Sidebar Start ========== -->
    <div class="left side-menu">
        <button type="button"
                class="button-menu-mobile button-menu-mobile-topbar open-left waves-effect">
            <i class="ion-close"></i>
        </button><!-- LOGO -->
        <div class="topbar-left">
            <div class="text-center bg-logo">
                <a asp-controller="Dashboard" asp-action="Dashboard" class="logo">
                    Kernel Travel Guide
                </a>
            </div>
        </div>
      
        <div class="sidebar-inner slimscrollleft">
            <div id="sidebar-menu">
                <ul>
                  
                    <li>
                            <a asp-controller="Dashboard" asp-action="Dashboard" class="waves-effect">
                            <i class="dripicons-device-desktop"></i>
                            <span>
                                Dashboard 
                            </span>
                        </a>
                    </li>
                    <li>
                            <a asp-controller="Feedbacks" asp-action="Index" class="waves-effect">
                                <span>
                                <i class="dripicons-user-group"></i>
                                User Feedback
                            </span>
                        </a>
                    </li>
                    <li>
                            <a asp-controller="Messages" asp-action="Index" class="waves-effect">
                                <span>
                                    <i class="dripicons-user"></i>
                                    User Messages
                                </span>
                            </a>
                        </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-world"></i> <span>City </span><span class="float-right">
                                <i class="mdi mdi-chevron-right"></i>
                            </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Cities" asp-action="Index">All Cities </a></li>
                                <li><a asp-controller="Cities" asp-action="Create">Add City </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-location-arrow"></i> <span>Country </span><span class="float-right">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Countries" asp-action="Index">All Country </a></li>
                                <li><a asp-controller="Countries" asp-action="Create">Add Country </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-home"></i><span> Hotels </span><span class="float-right">
                                <i class="mdi mdi-chevron-right"></i>
                            </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Hotels" asp-action="Index">All Hotel </a></li>
                                <li><a asp-controller="Hotels" asp-action="Create">Add Hotel </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-package"></i> <span>Resorts </span><span class="float-right">
                                <i class="mdi mdi-chevron-right"></i>
                            </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Resorts" asp-action="Index">All Resorts </a></li>
                                <li><a asp-controller="Resorts" asp-action="Create">Add Resort </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-gift"></i><span> Restaurants </span><span class="float-right">
                                <i class="mdi mdi-chevron-right"></i>
                            </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Restaurants" asp-action="Index">All Restaurants </a></li>
                                <li><a asp-controller="Restaurants" asp-action="Create">Add Restaurant </a></li>
                        </ul>
                        </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-flag-alt"></i>    <span> Travel Categories </span><span class="float-right">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="TravelCategories" asp-action="Index"> All Travel Categories </a></li>
                                <li><a asp-controller="TravelCategories" asp-action="Create">Add Travel Category </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                        <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-package"></i><span> Packages </span><span class="float-right">
                                <i class="mdi mdi-chevron-right"></i>
                            </span>
                        </a>
                        <ul class="list-unstyled">
                                <li><a asp-controller="Packages" asp-action="Index">All Packages </a></li>
                                <li><a asp-controller="Packages" asp-action="Create">Add Package </a></li>
                        </ul>
                    </li>
                    <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-direction-alt"></i> <span> Touriest Spots </span><span class="float-right">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                            </a>
                            <ul class="list-unstyled">
                                <li><a asp-controller="TouriestSpots" asp-action="Index"> All Touriest Spots </a></li>
                                <li><a asp-controller="TouriestSpots" asp-action="Create">Add Touriest Spot </a></li>
                            </ul>
                        </li>
                    <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="ti-truck"></i><span> Transport </span><span class="float-right">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                            </a>
                            <ul class="list-unstyled">
                                <li><a asp-controller="Transports" asp-action="Index">All Transports </a></li>
                                <li><a asp-controller="Transports" asp-action="Create">Add Transport </a></li>
                            </ul>
                        </li>
                </ul>
            </div>
            <div class="clearfix"></div>

        </div><!-- end sidebarinner -->
    </div>
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <!-- Top Bar Start -->
                <div class="topbar">
                    <nav class="navbar-custom">
                        <ul class="list-inline float-right mb-0">
                            <!-- language-->
                            
                            <li class="list-inline-item dropdown notification-list">
                                <a class="nav-link dropdown-toggle arrow-none waves-effect nav-user"
                                   data-toggle="dropdown" href="#" role="button" aria-haspopup="false"
                                   aria-expanded="false">
                                    <i class="ti-user  text-white fs-4"></i> Admin
                                </a>
                                <div class="dropdown-menu dropdown-menu-right profile-dropdown">
                                    <!-- item-->
                                    <div class="dropdown-item noti-title">
                                        <h5>Admin</h5>
                                    </div>
                                    <a class="dropdown-item" id="register" asp-area="Identity" asp-page="/Account/Register">
                                        <i class="mdi mdi-account-circle m-r-5 text-muted"></i> Create New User
                                    </a> 
                                    
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item" href="#">
                                
                                        <partial name="_DashboardLoginPartial"  class="nav-link  " />
                                    </a>
                                </div>
                            </li>
                        </ul>
                        <ul class="list-inline menu-left mb-0">
                            <li class="float-left">
                                <button class="button-menu-mobile open-left waves-light waves-effect">
                                    <i class="mdi mdi-menu"></i>
                                </button>
                            </li>
                            <li class="hide-phone app-search">
                                <form role="search" class="">
                                    <input type="text" placeholder="Search..."
                                           class="form-control"> <a href="#"><i class="fas fa-search"></i></a>
                                </form>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </nav>
                </div><!-- Top Bar End -->

    @RenderBody()
              
            </div>
        </div>
    </div>
    <footer class="footer">© 2023 Copyrights reserve by Kernel Travel Guide.</footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/dashboard/assets/js/jquery.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/popper.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/modernizr.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/detect.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/fastclick.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/jquery.slimscroll.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/jquery.blockUI.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/waves.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/jquery.nicescroll.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/js/jquery.scrollTo.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/plugins/chart.js/chart.min.js" asp-append-version="true"></script>
    <script src="~/dashboard/assets/pages/dashboard.js" asp-append-version="true"></script><!-- App js -->
    <script src="~/dashboard/assets/js/app.js" asp-append-version="true"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
